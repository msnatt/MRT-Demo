@model IEnumerable<MRT_Demo.Models.SEOPlan>

@{
    ViewBag.Title = "Recycle Bin";
}

<h2>กล่องขยะแผนวิสาหกิจ</h2>
<br />
<br />
<div class="row">
    <div class="offset-8 col-md-4 text-end">
        @Html.ActionLink("ย้อนกลับ", "Index", "SEOPlans", "", new { @class = "btn btn-secondary" })
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartEndYear)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UpdateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartEndYear)
        </td>
        <td>
            @Html.ActionLink("คืนค่า", "Revert", new { id =item.ID },new { @class = "btn" }) |
            @Html.ActionLink("ลบ", "LastDelete", new { id =item.ID }, new { @class = "btn" })
        </td>
    </tr>
}

</table>


<script>
    function ToStrategicObjectives(id, a) {
        $("#SEOform").attr("action", '@Url.Action("Index","StrategicObjectives")');
        $("#SEOform").submit();
    }
</script>
