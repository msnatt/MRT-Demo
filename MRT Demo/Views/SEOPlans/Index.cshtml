@model IEnumerable<MRT_Demo.Models.SEOPlan>

@{
    ViewBag.Title = "SOEPlan";
}
<h2>การจัดการแผนวิสาหกิจ</h2>
<br />
<br />
<div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-6">
    </div>
    <div class="col-md-4 text-end">
        @Html.ActionLink("เพิ่ม", "Create", "SEOPlans","", new { @class = "btn btn-primary col-md-2" })
        @Html.ActionLink("กล่องขยะ", "RecycleBin", "SEOPlans", "", new { @class = "btn btn-secondary col-md-3" })
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartEndYear)
        </th>
        <th>
            @Html.DisplayName("การจัดการ")
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UpdateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartEndYear)
        </td>
        <td>
            @Html.ActionLink("แก้ไข", "Edit", new { id =item.ID },new { @class = "btn" }) |
            @Html.ActionLink("วัตถุประสงค์เชิงยุทธศาสตร์", "Index", "StrategicObjectives", new { id = item.ID, a = "" }, new { @class = "btn" }) |
            @Html.ActionLink("ลบ", "DeleteConfirmed", new { id =item.ID }, new { @class = "btn" })
        </td>
    </tr>
}

</table>


<script>
    function ToStrategicObjectives(id, a) {
        $("#SEOform").attr("action", '@Url.Action("Index","StrategicObjectives")');
        $("#SEOform").submit();
    }
</script>
