@model MRT_Demo.Models.Tactic


@Html.HiddenFor(m => m.ID)
@Html.HiddenFor(m => m.IsLastDelete)
@Html.HiddenFor(m => m.No)
@Html.HiddenFor(m => m.UpdateDate)
@Html.HiddenFor(m => m.CreateDate)
@Html.HiddenFor(m => m.IsDelete)
@Html.HiddenFor(m => m.StategyID)
@Html.HiddenFor(m => m.CreateBy)
@Html.HiddenFor(m => m.UpdateBy)


@if (Model.IsDelete != true)
{
    <tr>
        <td>
            @Html.EditorFor(m => m.Tactic1, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Tactic1, "", new { @class = "text-danger" })
        </td>
        <td>
            <input value="ลบ" type="button" class="btn btn-danger" onclick="funcDelTactic(@Html.IdFor(m=>m.IsDelete))" />
        </td>
    </tr>
}
